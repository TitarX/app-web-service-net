<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="AppWebService" namespace="AppWebService.Models">
  <class name="Applications" table="Applications" dynamic-update="true" >
    <cache usage="read-write" />
    <id name="NumberApplication" column="NumberApplication" type="int">
      <generator class="native" />
    </id>
    <property name="DateApplication" />
    <property name="CompanyName" />
    <property name="UserName" />
    <property name="Position" />
    <property name="Email" />
  </class>
</hibernate-mapping>
